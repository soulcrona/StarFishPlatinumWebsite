@model IEnumerable<StarFishWebsite.Models.FishType>

@{
    ViewBag.Title = "FishType";
}

<h2>Fish Types</h2>

@Html.ActionLink("Add Fish Type", "NewFishType", "Upload", "", new { @class = "btn btn-primary" })

<div>
    <table>
        <tr>
            <th>Name:</th>
            <th></th>
        </tr>
        @foreach (var item in Model)
        {
            if (item.Available == true)
            {
                <tr>
                    <td>@item.Name</td>
                    <td>
                        @Html.ActionLink("Edit", "FishType", "Edit", new { item.ID }, new { @class = "btn btn-default" })
                        @Html.ActionLink("Delete", "SoftDeleteFishType", "Edit", new { item.ID }, new { @class = "btn btn-default" })
                    </td>
                </tr>
            }
        }
    </table>
</div>
