@model StarFishWebsite.Models.FoodType

@{
    ViewBag.Title = "NewFoodType";
}
<div class="card bg-light mb-3">
    <div class="card-header">
        <h2>New Food Type</h2>
    </div>
    <div class="card-body">
        @using (Html.BeginForm("AddFoodType", "Upload", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <table>
                <tr>
                    <td>
                        @Html.LabelFor(u => u.Name)</td><td>
                        @Html.TextBoxFor(u => u.Name, new { @class = "form-control" })
                    </td>
                </tr>
                <tr>
                    <td></td>
                    <td>
                        <button type="submit" class="btn btn-primary">Add</button>
                    </td>
                </tr>
            </table>
        }
</div>
</div>