@model StarFishWebsite.Models.FishType

@{
    ViewBag.Title = "AddFishType";
}

<h2>AddFishType</h2>


@using (Html.BeginForm("AddFishType", "Upload", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="form-group">
        @Html.LabelFor(u => u.Name)
        @Html.TextBoxFor(u => u.Name, new { @class = "form-control" })
    </div>
    <br />
    <button type="submit" class="btn btn-primary">Edit</button>
}

